msgstr "Kliknutím na tlačítko <gui>Předchozí stránka</gui> na <gui>panelu nástrojů</gui>."

#: C/photos_by_page.page:36(p)
msgid "Press <keyseq><key>Alt</key><key>Page Up</key></keyseq>."
msgstr "Zmáčknutím <keyseq><key>Alt</key><key>Page Up</key></keyseq>."

#: C/photos_by_page.page:39(p)
msgid "Choose <guiseq><gui>Go</gui><gui>Previous Page</gui></guiseq> from the <gui>Main Window</gui>."
msgstr "Volbou <guiseq><gui>Přejít</gui><gui>Předchozí stránka</gui></guiseq> v <gui>hlavním okně</gui>."

