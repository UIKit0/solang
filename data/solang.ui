<!--*- xml -*-->
<ui>
	<menubar name="MenuBar">
		<menu action="ActionPhotoMenu">
			<placeholder name="PlaceholderImportMenus"/>
			<placeholder name="PlaceholderExportMenus"/>
			<separator/>
			<placeholder name="PlaceholderPrintMenus"/>
			<separator/>
			<menuitem action="ActionPhotoQuit"/>
		</menu>
		<menu action="ActionViewMenu">
			<menuitem action="ActionViewToolBar"/>
			<menuitem action="ActionViewStatusBar"/>
			<separator/>
			<menuitem action="ActionViewFullScreen"/>
		</menu>
		<placeholder name="PlaceholderViewMenus"/>
		<placeholder name="PlaceholderFindMenus"/>
		<placeholder name="PlaceHolderTagMenus"/>
