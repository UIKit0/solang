	</menubar>
	<toolbar name="ToolBar">
		<placeholder name="PlaceholderImportToolBar"/>
		<placeholder name="PlaceholderExportToolBar"/>
		<separator/>
		<toolitem action="ActionViewFullScreen"/>
	</toolbar>
</ui>
